  "scripts": {
    "cli-help": "tsx ./src/cli.ts",
    "cli-help2": "tsx ./src/cli.ts --no-color",
    "cli-help3": "tsx ./src/cli.ts /?",
    "cli-version": "tsx ./src/cli.ts -v",
    "cli-validate1": "tsx ./src/cli.ts ./tests/data/InvalidModel.lhq validate --verbose",
    "cli-validate2": "tsx ./src/cli.ts ./tests/data/NetCoreResxCsharp01.lhq validate",
    "cli-wrong1": "tsx ./src/cli.ts generate validate file.lhq",
    "cli-1": "tsx ./src/cli.ts ./tests/data/NetCoreResxCsharp01.lhq --project MyProj.csproj -d key1=value1 --data k2=v2",
    "cli-2": "tsx ./src/cli.ts ./tests/data/NetCoreResxCsharp01.lhq -o temp/cli --verbose",
    "cli-3": "tsx ./src/cli.ts c:/Temp/neo_online/Neo.Localization/Strings.lhq -o temp/cli",
    "cli-4": "tsx ./src/cli.ts c:/dev/github/psulek/lhqeditor/src/Gen.Lib.Tests/TestData/NetCoreResxCsharp01/Strings.lhq",
    "cli-5": "tsx ./src/cli.ts ./tests/data/NetCoreResxCsharp01.lhq -o temp/cli -d namespace=Root a=2 xyz=Peter --no-colors",
    "publish-local": "pnpm run build-all && cd dist && pnpm publish --registry http://localhost:4873/ --no-git-checks && rimraf dist",
    "unpublish-local": "pnpm unpublish lhq-generators --registry http://localhost:4873/ --force",
    "build-all": "rimraf dist && tsx ./build.ts",
    "eslint": "eslint",
    "test": "mocha --delay ./tests/index.spec.ts"
  },
