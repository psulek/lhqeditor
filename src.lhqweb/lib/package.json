    "zod": "^3"
  },
  "devDependencies": {
    "@duplojs/zod-to-typescript": "0.3.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/get-value": "3.0.5",
    "@types/jmespath": "0.15.2",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.10.10",
    "cpx": "1.5.0",
    "fs-extra": "11.3.0",
    "minimist": "1.2.8",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.3",
    "zod-to-ts": "^1.2.0",
    "zod-validation-error": "3.4.0"
  }
