{
  "name": "lhq-generators",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gen-schema": "ts-node --project tsconfig.build.json ./src/build/cli.ts --gen-schema",
    "validate-schema": "ts-node --project tsconfig.build.json ./src/build/cli.ts --validate-schema data/WinFormsResxCsharp01.lhq",
    "gen-lhq": "ts-node --project tsconfig.build.json ./src/build/cli.ts --gen-lhq data/WinFormsResxCsharp01.lhq",
    "gen-lhq2": "ts-node --project tsconfig.build.json ./src/build/cli.ts --gen-lhq data/NetCoreResxCsharp01.lhq",
    "__gen-dts": "ts-node --project tsconfig.build.json ./src/build/cli.ts --gen-dts",
    "test": "ts-node --project tsconfig.build.json ./src/build/test.ts",
    "build": "rimraf dist && webpack",
    "dts": "tsup src/model/api/index.ts --tsconfig tsconfig.dts.json --dts-only -d ./dist",
    "pkg": "rimraf dist && npm run dts && cpx package.json dist && cd dist && npm pack"
  },
