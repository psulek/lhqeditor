{
  "name": "@lhq/lhq-generators",
  "version": "1.0.9",
  "author": "Peter Å ulek",
  "description": "LHQ generators library",
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "build-lib": "rimraf dist && tsup --config tsup.lib.ts",
    "build-cli": "rimraf dist && tsup --config tsup.cli.mts",
    "cli-1": "ts-node --project tsconfig.build.json ./build/cli.mts file.lhq generate --project MyProj.csproj -d key1=value1 --data k2=v2",
    "cli-2": "ts-node --project tsconfig.build.json ./build/cli.mts c:/dev/github/psulek/lhqeditor/src/Gen.Lib.Tests/TestData/WinFormsResxCsharp01/Strings.lhq --project MyProj.csproj -d key1=value1 --data k2=v2",
    "cli-3": "ts-node --project tsconfig.build.json ./build/cli.mts c:/dev/github/psulek/lhqeditor/src/Gen.Lib.Tests/TestData/WinFormsResxCsharp01/Strings.lhq generate -o temp/cli",
    "cli-4": "ts-node --project tsconfig.build.json ./build/cli.mts c:/dev/github/psulek/lhqeditor/src/Gen.Lib.Tests/TestData/WinFormsResxCsharp01/Strings.lhq -o temp/cli -d namespace=Root",
    "cli-5": "ts-node --project tsconfig.build.json ./build/cli.mts c:/dev/github/psulek/lhqeditor/src/Gen.Lib.Tests/TestData/WinFormsResxCsharp01/Strings.lhq validate",
    "cli-help": "ts-node --project tsconfig.build.json ./build/cli.mts",
    "cli-wrong1": "ts-node --project tsconfig.build.json ./build/cli.mts generate validate file.lhq",
    "genarate-lhq": "ts-node --project tsconfig.build.json ./build/cli.mts --gen-lhq data/WinFormsResxCsharp01.lhq",
    "publish-local00": "cd dist && pnpm publish --registry http://localhost:4873/",
    "publish-local": "pnpm run build-all && cd dist && pnpm publish --registry http://localhost:4873/ --no-git-checks",
    "unpublish-local": "pnpm unpublish lhq-generators --registry http://localhost:4873/ --force",
    "build-all": "ts-node-esm --project tsconfig.build.json ./build.mts",
    "eslint": "eslint"
  },
  "__scripts": {
    "_gen-schema": "ts-node --project tsconfig.build.json ./build/cli.mts --gen-schema",
    "gen-schema": "bun run ./build/cli.mts --gen-schema",
    "validate-schema": "ts-node --project tsconfig.build.json ./build/cli.mts --validate-schema data/WinFormsResxCsharp01.lhq",
    "gen-lhq2": "ts-node --project tsconfig.build.json ./build/cli.mts --gen-lhq data/NetCoreResxCsharp01.lhq",
    "__gen-dts": "ts-node --project tsconfig.build.json ./build/cli.mts --gen-dts",
    "test": "ts-node --project tsconfig.build.json ./build/test.ts",
    "build": "rimraf dist && webpack",
    "dts": "tsup src/model/api/index.ts --tsconfig tsconfig.dts.json --dts-only -d ./dist",
    "pkg": "rimraf dist && npm run dts && cpx package.json dist && cd dist && npm pack"
  },
