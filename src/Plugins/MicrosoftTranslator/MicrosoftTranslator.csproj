    <RootNamespace>LHQ.Plugin.MicrosoftTranslator</RootNamespace>
    <AssemblyName>LHQ.Plugin.MicrosoftTranslator</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Configurations>AppSetup;Debug;Release</Configurations>
